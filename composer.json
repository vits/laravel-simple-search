{
    "name": "vits/laravel-simple-search",
    "description": "Simple search scope for Laravel models.",
    "type": "library",
    "license": "MIT",
    "version": "0.1",
    "autoload": {
        "psr-4": {
            "Vits\\LaravelSimpleSearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vits\\LaravelSimpleSearch\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Vitauts Stoƒçka",
            "email": "vs@vits.lv"
        }
    ],
    "require": {
        "php": "~8.2",
        "illuminate/database": "^10.0|^11.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.2",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-watch": "^2.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "scripts": {
        "test": [
            "vendor/bin/pest"
        ]
    }
}